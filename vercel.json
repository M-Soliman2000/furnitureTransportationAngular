{
  "version": 2,
  "buildCommand": "npm run build:ssr",
  "outputDirectory": "dist/furniture-transportation-angular",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|otf|map|txt|xml))",
      "dest": "/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.mjs"
    }
  ]
}