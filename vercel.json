{
  "version": 2,
  "builds": [
    {
      "src": "dist/furniture-transportation-angular/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/furniture-transportation-angular/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/dist/furniture-transportation-angular/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/furniture-transportation-angular/server/main.js"
    }
  ]
}